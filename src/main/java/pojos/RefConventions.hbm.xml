<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Created by charbda 20/04/2011 -->
<hibernate-mapping>
  <class name="pojos.RefConventions" table="ref_conventions">
    <id name="refConventionsId" type="int">
      <column name="REF_CONVENTIONS_ID"/>
      <generator class="increment"/>
    </id>
    <property name="refConventionsName" type="string">
      <column length="64" name="REF_CONVENTIONS_NAME"/>
    </property>
    <property name="refConventionsCode" type="string">
      <column length="64" name="REF_CONVENTIONS_CODE"/>
    </property>
  </class>
</hibernate-mapping>
