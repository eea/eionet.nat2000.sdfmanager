<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Created by charbda 20/04/2011 -->
<hibernate-mapping>
  <class name="pojos.RefPopulation" table="ref_population_type">
    <id name="refPopulationId" type="int">
      <column name="POPULATION_ID"/>
      <generator class="increment"/>
    </id>
    <property name="refPopulationName" type="string">
      <column length="256" name="POPULATION_NAME"/>
    </property>
    <property name="refPopulationCode" type="string">
      <column length="1" name="POPULATION_CODE"/>
    </property>
  </class>
</hibernate-mapping>