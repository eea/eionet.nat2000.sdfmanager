<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Created by charbda 20/04/2011 -->
<hibernate-mapping>
  <class catalog="natura2000" name="pojos.RefQuality" table="ref_data_quality">
    <id name="refQualityId" type="int">
      <column name="QUALITY_ID"/>
      <generator class="increment"/>
    </id>
    <property name="refQualityName" type="string">
      <column length="256" name="QUALITY_NAME"/>
    </property>
    <property name="refQualityCode" type="string">
      <column length="2" name="QUALITY_CODE"/>
    </property>
    <property name="refQualitySpecies" type="string">
      <column length="1" name="QUALITY_SPECIES"/>
    </property>
  </class>
</hibernate-mapping>