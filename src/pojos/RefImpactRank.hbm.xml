<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Created by charbda 20/04/2011 -->
<hibernate-mapping>
  <class catalog="natura2000" name="pojos.RefImpactRank" table="ref_impact_rank">
    <id name="refRankId" type="int">
      <column name="RANK_ID"/>
      <generator class="increment"/>
    </id>
    <property name="refRankName" type="string">
      <column length="256" name="RANK_NAME"/>
    </property>
    <property name="refRankCode" type="string">
      <column length="1" name="RANK_CODE"/>
    </property>
  </class>
</hibernate-mapping>